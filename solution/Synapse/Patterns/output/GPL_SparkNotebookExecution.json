          "snapshot": true,
          "sparkPool": {
            "referenceName": "arkstgsynspads",
            "type": "BigDataPoolReference"
          }
        },
        "userProperties": []
      }
    ],
    "annotations": [],
    "folder": {
      "name": "ADS Go Fast/Azure/ErrorHandler/"
